{
  "name": "genz-alpha-news-aggregator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node generateStaticArticles.mjs",
    "postbuild": "node updateSitemap.mjs && node checkRobotsTxt.mjs",
    "generate-static": "node generateStaticArticles.mjs",
    "update-sitemap": "node updateSitemap.mjs",
    "check-robots": "node checkRobotsTxt.mjs",
    "seo-check": "npm run generate-static && npm run update-sitemap && npm run check-robots",
    "fetch-rss": "node processRSSFeeds.mjs",
    "schedule-rss": "node rssScheduler.mjs",
    "analyze-rss": "node analyzeFeedPatterns.mjs",
    "test-counter": "node test_article_counter.mjs",
    "update-stats": "node processRSSFeeds.mjs && node updateSitemap.mjs",
    "deploy-stats": "sh deploy_article_stats.sh",
    "deploy-rss": "sh deploy_rss_system.sh install",
    "deploy-git": "sh deploy_git_rss.sh",
    "deploy-win": "powershell -ExecutionPolicy Bypass -File .\\Deploy-GitRSS.ps1",
    "verify": "sh verify_deployment.sh",
    "verify-win": "powershell -ExecutionPolicy Bypass -File .\\Verify-Deployment.ps1",
    "git-push": "sh git_push.sh",
    "git-push-win": "powershell -ExecutionPolicy Bypass -File .\\Git-Push.ps1",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.344.0",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-intersection-observer": "^9.5.3",
    "react-router-dom": "^6.20.1",
    "react-share": "^5.0.3",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
